package org.vote

trait RecountSystem {
 // Plurality voting system
  
  def theWinnerIs() : (AnyRef,Integer) = getWithMoreVotes(votationResults())
  
  def getWithMoreVotes(elems : List[(AnyRef,Integer)]) : (AnyRef, Integer)
  def votationResults() : List[(AnyRef,Integer)]
  def countOcurrencesOf(E:Vote, list:List[_],Count:Integer) : (AnyRef,Integer) 
}